{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "A nest stater project",
  "author": "cwuyiqing@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker": "docker build . -t nest-app",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "8.2.0",
    "@nestjs/core": "8.2.0",
    "@nestjs/platform-express": "8.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.4",
    "@types/express": "4.17.13",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-alloy": "4.3.0",
    "prettier": "2.4.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.5.2"
  }
}
